/* == Control Panel == */
#controlpanel {
	position:fixed;
	background:url(../images/footer-bg.png) 0 0;
	width:100px;
	padding:10px;
	left:20px;
	top:20px;
	color:#fff;
	z-index:3;
	border:1px solid #585858;
}

#controlpanel ul {
	list-style:none;
}

#controlpanel strong { clear:both; display:block; padding-top:10px; }

